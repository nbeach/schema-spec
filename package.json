{
  "name": "schema-spec",
  "version": "0.1.1",
  "description": "A JavaScript library for defining object schemas and validating object against them.",
  "main": "dist/schema-spec.js",
  "scripts": {
    "build": "gulp",
    "build:watch": "gulp watch",
    "test": "mocha --recursive -R spec",
    "test:watch": "mocha --recursive -w -R min",
    "coverage": "istanbul cover _mocha",
    "ci-coverage": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nbeach/schema-spec.git"
  },
  "author": "Nicholas Beach <nicholas.c.beach@gmail.com> (http://nicholasbeach.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nbeach/schema-spec/issues"
  },
  "keywords": ["validation", "schema", "specification", "object", "spec", "valid"],
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-jsdoc-to-markdown": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5"
  }
}
